{
  "name": "virtualized-ui",
  "version": "0.2.0",
  "description": "Headless virtualized table, list, and select components for React",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "react",
    "virtual",
    "virtualized",
    "table",
    "list",
    "select",
    "headless",
    "tanstack",
    "react-table",
    "react-virtual"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/grishalr/virtualized-ui.git",
    "directory": "packages/core"
  },
  "homepage": "https://virtualized-ui.dev",
  "bugs": {
    "url": "https://github.com/grishalr/virtualized-ui/issues"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "@tanstack/react-table": "^8.20.0",
    "@tanstack/react-virtual": "^3.10.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.2",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^5.1.2",
    "jsdom": "^27.4.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "tsup": "^8.3.0",
    "typescript": "^5.6.0",
    "vitest": "^4.0.18"
  }
}
